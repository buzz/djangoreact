{
  "name": "djangoreact",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "config": {
    "debug": "true",
    "wagtail-port": 8000,
    "render-port": 8001,
    "webpack-port": 8002,
    "api-base-path": "api/v2/",
    "api-pages-path": "pages",
    "admin-base-path": "admin/",
    "virtualenv-dir": "server/.virtualenv/",
    "static-root": "server/assets-collected",
    "media-root": "server/media"
  },
  "scripts": {
    "clean": "rm -rf $npm_package_config_static_root",
    "dev": "concurrently --prefix '[{name}]' --names 'WEBPACK,RENDER,WAGTAIL' -c 'red,yellow,blue' --kill-others 'npm run dev:webpack' 'npm run dev:render-server' 'npm run dev:wagtail'; npm run dev:kill",
    "dev:wagtail": "source $npm_package_config_virtualenv_dir/bin/activate && PYTHONUNBUFFERED=true python server/manage.py runserver localhost:$npm_package_config_wagtail_port",
    "dev:webpack": "webpack-dev-server --config webpack/dev.config.babel.js --port $npm_package_config_webpack_port --quiet",
    "dev:render-server": "NODE_PATH=client api_pages_url=\"http://127.0.0.1:${npm_package_config_wagtail_port}/${npm_package_config_api_base_path}${npm_package_config_api_pages_path}/\" nodemon --no-stdin --exec 'babel-node --plugins transform-react-jsx' -e js,jsx -w ./server/js/ -w ./client/ server/js/main.js --port $npm_package_config_render_port",
    "dev:kill": "ps -ef|grep -e '\\(concurrently|webpack-dev-server\\)'|cut -d' ' -f6|xargs -L1 kill -9 2>/dev/null;true",
    "wagtail:manage": "source $npm_package_config_virtualenv_dir/bin/activate && python server/manage.py",
    "build": "npm run clean && npm run build:webpack && npm run build:wagtail",
    "build:webpack": "webpack -p --config webpack/prod.config.babel.js",
    "build:wagtail": "source $npm_package_config_virtualenv_dir/bin/activate && python server/manage.py collectstatic --no-input",
    "test": "NODE_PATH=client babel-node --plugins transform-react-jsx -- server/js/test.js",
    "eslint": "eslint --ext jsx client",
    "create-virtualenv": "virtualenv --python=python3.6 $npm_package_config_virtualenv_dir && source $npm_package_config_virtualenv_dir/bin/activate && pip install -r server/requirements.txt"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "buzz",
  "license": "GPL-2.0",
  "dependencies": {
    "array.prototype.find": "^2.0.4",
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^4.0.0-alpha.6",
    "css-loader": "^0.26.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.1",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.17",
    "postcss-apply": "^0.6.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-responsive-type": "^0.5.1",
    "postcss-smart-import": "^0.6.12",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-render": "^1.2.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-api": "^0.10.3",
    "redux-saga": "^0.14.7",
    "redux-thunk": "^2.2.0",
    "redux-watch": "^1.1.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "tether": "^1.4.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.11.0",
    "redux-devtools-extension": "^2.13.0",
    "style-loader": "^0.13.2",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.4.3"
  }
}
