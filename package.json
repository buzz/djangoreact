{
  "name": "djangoreact",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "config": {
    "webpack-port": 3000,
    "django-port": 8000,
    "django-virtualenv-dir": "server/.virtualenv/",
    "django-static-root": "server/assets-collected",
    "django-media-root": "server/media"
  },
  "scripts": {
    "clean": "rm -rf $npm_package_config_django_static_root",
    "dev": "concurrently --kill-others \"npm run dev:django\" \"npm run dev:webpack\"",
    "dev:django": "source $npm_package_config_django_virtualenv_dir/bin/activate && DJANGO_DEBUG=True python server/manage.py runserver localhost:$npm_package_config_django_port",
    "dev:webpack": "webpack-dev-server --config webpack/dev.config.babel.js --hot --inline --watch-content-base --watch --port $npm_package_config_webpack_port",
    "django:manage": "source $npm_package_config_django_virtualenv_dir/bin/activate && python server/manage.py",
    "build": "npm run clean && npm run build:webpack && npm run build:django",
    "build:webpack": "webpack -p --config webpack/prod.config.babel.js",
    "build:django": "source $npm_package_config_django_virtualenv_dir/bin/activate && python server/manage.py collectstatic --no-input",
    "eslint": "eslint --ext jsx client",
    "create-virtualenv": "virtualenv --python=python3.6 $npm_package_config_django_virtualenv_dir && source $npm_package_config_django_virtualenv_dir/bin/activate && pip install -r server/requirements.txt"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "buzz",
  "license": "GPL-2.0",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.23.0",
    "bootstrap": "^4.0.0-alpha.6",
    "css-loader": "^0.26.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.1",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.16",
    "postcss-apply": "^0.6.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-responsive-type": "^0.5.1",
    "postcss-smart-import": "^0.6.10",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-api": "^0.10.3",
    "redux-saga": "^0.14.7",
    "redux-thunk": "^2.2.0",
    "redux-watch": "^1.1.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "tether": "^1.4.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "react-hot-loader": "^1.3.1",
    "redux-devtools-extension": "^2.13.0",
    "style-loader": "^0.13.2",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-server": "^2.4.1"
  }
}
